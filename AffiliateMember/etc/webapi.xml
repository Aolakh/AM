<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/affiliatemembers/" method="GET">
        <service class="AM\AffiliateMember\Api\AffiliateRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/searchaffiliatemembers" method="GET">
        <service class="AM\AffiliateMember\Api\AffiliateRepositoryInterface" method="getMemberList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>